<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asuna.ssm.mapper.UserMapper">

    <select id="checkUser" resultType="User">
        select * from user_asuna where user_name = #{userName} and user_password = #{userPassword}
    </select>

</mapper>
